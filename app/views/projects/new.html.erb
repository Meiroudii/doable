<h1>New Project</h1>

<%= form_with model: @project do |f| %>
  <fieldset>
    <legend>Planning a new project lad?</legend>
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <div class="actions">
      <%= f.submit "Create Project" %>
    </div>
  </fieldset>
<% end %>
